{
    "extends": [
        "plugin:prettier/recommended",
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "plugins": [
        "prettier",
        "jest"
    ],
    "rules": {
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "tabWidth": 4,
            "bracketSpacing": false
        }]
    }
}
